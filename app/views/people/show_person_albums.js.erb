content = "<%= escape_javascript(render('albums/albums_container')) %>";
page = '<%=@page%>';
person_id = "<%=@person.id%>";
if (page == ''){ 
  $('.stream').html(content);
}else{
  $('.stream').append(content);
}
tab = '<%=@tab%>';
if(tab != ''  ){
  $( '.current' ).removeClass( 'current' );
  $( '#'+tab).addClass( 'current' );
}
$(".tab_progress").hide();
$( "#my_albums" ).attr( "href","/people/"+person_id+"/show_person_albums" );




