content = "<%= escape_javascript(render('status_messages/status_messages_container')) %>"; 
page = '<%=@page%>';
if (page == ''){ 
  $('.stream').html(content);
}else{
  $('.stream').append(content);
}
tab = '<%=@tab%>';
if(tab != ''  ){
  $( '.current' ).removeClass( 'current' );
  $( '#'+tab).addClass( 'current' );
}
$("abbr.timeago").timeago();




