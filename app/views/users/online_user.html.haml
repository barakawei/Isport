.online_user
  %h2
    = @online_user.size
    = "在线用户"
    = link_to "返回",site_admin_path
  -if @online_user.empty?
    %p
      ="无"
  -else
    .person_details_content      
      = render :partial=>'people/person_element',:collection => @online_user,:as => :person
    .pagination_box
      - if !@online_user.nil? && @online_user.size > 0
        = will_paginate @online_user, {:previous_label => t("pagination.previous"),
                               :next_label => t("pagination.next")}
