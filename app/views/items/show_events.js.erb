content = "<%= escape_javascript(render('people/person_event_container')) %>";
page = '<%=@page%>';
item_id = "<%=@item.id%>";
if (page == ''){ 
  $('.stream').html(content);
}else{
  $('.stream').append(content);
}
tab = '<%=@tab%>';
if(tab != ''  ){
  $( '.current' ).removeClass( 'current' );
  $( '#'+tab).addClass( 'current' );
}
$(".tab_progress").hide();
$( "#item_events" ).attr( "href","/items/"+item_id+"/show_events");




