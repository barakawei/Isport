.span-3
  - if event.image_url_medium
    = link_to image_tag(event.image_url_medium), event 
.span-9.last.list
  %h3= link_to event.title, event, :class => "nounderline"
  %p#notice= notice
  %p
    %b= t('events.start_at')+":"
    = event.start_at
  %p
    %b= t('events.nd_at')+":"
    = event.end_at
  %p
    %b= t('events.location')+":"
    = event.location
  %p
    %b= t('events.event_subject')+":"
    = event.subject_id
